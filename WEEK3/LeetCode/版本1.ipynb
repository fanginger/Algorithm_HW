class Node:
    def __init__(self, data, next=None):
        self.data = data
        self.next = next
    def __repr__(self):
        return str(self.data)

class LinkList:
    def __init__(self):
        self.head = None    

    def print(self):
        curr = self.head
        while curr:
            print(curr)
            curr = curr.next

    def addTail(self, node):
        if self.head == None:
            self.head = node
        else:
             curr = self.head
             while curr:
                 if curr.next != None:
                    curr = curr.next
                 else:   
                    curr.next = node
                    return

    def addHead(self, node):
        if self.head == None:
            self.head = node
        else:
            curr = self.head
            self.head = node
            self.head.next = curr

    def addAtIndex(self, index, node):
        if index < 0:
